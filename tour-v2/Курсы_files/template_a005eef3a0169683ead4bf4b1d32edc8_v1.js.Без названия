$(document).on('submit', '#userData', function(){    
    var form_data = $(this).serialize();
    $.ajax({
        type: 'POST',
        data: form_data,
        success: function(html){
            $(".user--ajax").html($(html).find(".user"));
            $(".popup__user--ajax").html($(html).find(".popup__content--user"));
            $(".top .top__authorized--ajax").html($(html).find(".top .top__authorized"));
            $(".top--mobile .top__authorized--ajax").html($(html).find(".top--mobile .top__authorized"));
        }
    })
    return false
});


