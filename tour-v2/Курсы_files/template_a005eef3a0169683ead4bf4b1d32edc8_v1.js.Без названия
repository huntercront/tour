        type: 'POST',
        data: form_data,
        success: function(html){
            $(".popup__static--ajax").html($(html).find(".popup__content--static"));
            $(".statistics--ajax").html($(html).find(".statistics__wrapper--ajax"));
            
        }
    })
    return false
});




