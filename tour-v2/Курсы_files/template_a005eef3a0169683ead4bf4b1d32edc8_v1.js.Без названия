    var form_data = $(this).serialize();
    $.ajax({
        type: 'POST',
        data: form_data,
        success: function(html){
            $(".popup__user--telegajax").html($(html).find(".popup__content--teleguser"));
        }
    })
    return false
});


$(document).on('submit', '#addUser', function(){    
    var form_data = $(this).serialize();
    $.ajax({
        type: 'POST',
        data: form_data,
        success: function(html){
            $(".popup__user--ajax").html($(html).find(".popup__user--messag"));
            $(".user__list--ajax").html($(html).find(".user__list__block"));
        }
    })
    return false
});

$(document).on('keyup', '.filter_users__input', function(){
    
	 login = $("#filter_users__login").val();
     call = $("#filter_users__call").val();
     
	 $.ajax({
		type: 'POST',
		data: {
			login: login,
            call: call
		},
		success: function(html){
			$(".user__list--ajax").html($(html).find(".user__list__block"));   
		}
	}) 
});


$(document).on('click', '.user__element__prodlit', function(){  
    
    id = $(this).data("id");
    email = $(this).data("email");
    name = $(this).data("name");
    srok = $(this).data("srok");
    idkurs = $(this).data("idkurs");
    
    $.ajax({
        type: 'POST',
        data: {
			id: id,
            email: email,
            name: name,
            srok: srok,
            idkurs: idkurs
		},
        success: function(html){
            $(".user__element--"+id).html($(html).find("#ajax--"+id));
        }
    })
});


$(document).on('click', '.user__element__kupit', function(){  
    idcat = $(this).data("idcat");
    email = $(this).data("email");
    name = $(this).data("name");
    time = $(this).data("time");
    idkurs = $(this).data("idkurs");
    $.ajax({
        type: 'POST',
        data: {
			idcat: idcat,
            email: email,
            name: name,
            time: time,
            idkurs: idkurs
		},
        success: function(html){
            $("#"+idcat).html($(html).find("#"+ idcat +" .user__list__wrapper"));
        }
    })
});


$(document).on('click', '.pitanie__top__item', function(){ 
    $(".pitanie__top__item").removeClass("pitanie__top__item--active");
    $(".pitanie__bottom__block").removeClass("pitanie__bottom__block--active");
    $blockActive = ".pitanie__bottom__block--"+$(this).data("catid");
    $(this).addClass("pitanie__top__item--active");
    $($blockActive).addClass("pitanie__bottom__block--active");
    
    podid = $($blockActive + " .pitanie__bottom__item--active").data("podid");
    catNedel = $(this).data("catid");
    
    
    $.ajax({
        type: 'POST',
        data: {
			IDCAT: podid,
            IDCATNDEL: catNedel
		},
        success: function(html){
            $(".pitanie__ajax").html($(html).find(".pitanie__wrapper"));
            $(".pitanie__right--ajax").html($(html).find(".pitanie__right__block"));
            $(".pitanie__top__cat--ajax").html($(html).find(".pitanie__top__cat--small"));
        }
    })
    
});


$(document).on('click', '.pitanie__bottom__item', function(){  

    $cat = $(".pitanie__top__item--active").data("catid");
    $blockActive = ".pitanie__bottom__block--" + $cat;

    $($blockActive + " .pitanie__bottom__item").removeClass("pitanie__bottom__item--active");
    $(this).addClass("pitanie__bottom__item--active");
    
    podid = $(this).data("podid");
    
    $.ajax({
        type: 'POST',
        data: {
			IDCAT: podid
		},
        success: function(html){
            $(".pitanie__ajax").html($(html).find(".pitanie__wrapper"));
            //$(".pitanie__right--ajax").html($(html).find(".pitanie__right__block"));
        }
    })
});


