$(function(){
   
var clicks = 0;   
$('.top__bell__colocol').on('click', function() {
    if (clicks == 0){
        $(".top__contmenu").removeClass('top__contmenu--active');
        $(".top__authorized").removeClass('top__authorized--active');
        $('.top__bell__block').toggleClass('top__bell__block--active');
        $('.top__bell__colocol').toggleClass('top__bell__colocol--active');
        clicks++;
    } else {
       $(".top__contmenu").removeClass('top__contmenu--active');
        $(".top__authorized").removeClass('top__authorized--active');
        $('.top__bell__block').removeClass('top__bell__block--active');
        $('.top__bell__colocol').removeClass('top__bell__colocol--active');
        clicks--;
    }
    
});

    $(document).on('click', '.get__srok__item ', function(){
        sales = $(this).data("sales");
        srock = $(this).data("moth");
        $(".get__srok__item").removeClass("get__srok__item--active");
        $(this).addClass("get__srok__item--active");
        $(".get__total__value").text(numberWithSpaces(sales));
        $(".get__total__value").data('pricefull',sales);
        $("#prices").val(sales);
        $("#srok").val(srock);
        $(".price__moth").text(numberWithSpaces(sales/srock))
        
    });


var clicks1 = 0;   
$('.top__authorized').on('click', function() {
    if (clicks1 == 0){
    
        if($(".top__contmenu").hasClass('top__contmenu--active') && $(this).hasClass('top__authorized--active')){
            $(".top__contmenu").removeClass('top__contmenu--active');
        $(this).removeClass('top__authorized--active');
        } else {
            $(".top__contmenu").addClass('top__contmenu--active');
            $(this).addClass('top__authorized--active');
        }
        
        $('.top__bell__block').removeClass('top__bell__block--active');
        $('.top__bell__colocol').removeClass('top__bell__colocol--active');
        
       clicks1++;
    } else {
    
      if($(".top__contmenu").hasClass('top__contmenu--active') && $(this).hasClass('top__authorized--active')){
            $(".top__contmenu").removeClass('top__contmenu--active');
        $(this).removeClass('top__authorized--active');
        } else {
            $(".top__contmenu").addClass('top__contmenu--active');
            $(this).addClass('top__authorized--active');
        }
        
        $('.top__bell__block').removeClass('top__bell__block--active');
        $('.top__bell__colocol').removeClass('top__bell__colocol--active');
        clicks1--;
    }
});



$('.top__authorized').hover(
  function() {
    $(".top__contmenu").addClass('top__contmenu--active');
    $(this).addClass('top__authorized--active');
    $('.top__bell__block').removeClass('top__bell__block--active');
    $('.top__bell__colocol').removeClass('top__bell__colocol--active');
  }, function() {
    
  }
)


    
$('.video__sticky').on('click', function() {
    $('.video__spisok').toggleClass('video__spisok--active');
    $(this).addClass("video__sticky--no");
    $('body').addClass('body--actives');
});

$('.video__mobile__top').on('click', function() {
    $('.video__spisok').toggleClass('video__spisok--active');
    $('body').addClass('body--actives');
});

$('.video__spisok__top').on('click', function() {
    $('.video__spisok').toggleClass('video__spisok--active');
    $('.video__sticky').removeClass("video__sticky--no");
    $('body').removeClass('body--actives');
});


$(document).mouseup(function (e){ 
    var div = $(".top__contmenu"); 
    var div2 = $(".top__authorized"); 
    if (!div.is(e.target) && div.has(e.target).length === 0 && !div2.is(e.target) && div2.has(e.target).length === 0) {
        $(".top__contmenu").removeClass('top__contmenu--active');
        $(".top__authorized").removeClass('top__authorized--active');
         clicks1 = 0;
    }
});

$(document).mouseup(function (e){ 
    var div = $(".top__bell__block"); 
    var div2 = $(".top__bell__colocol ");
    
    if (!div.is(e.target) && div.has(e.target).length === 0 && !div2.is(e.target) && div2.has(e.target).length === 0) {
        $('.top__bell__block').removeClass('top__bell__block--active');
        $('.top__bell__colocol').removeClass('top__bell__colocol--active');
        clicks = 0;
    }
});


