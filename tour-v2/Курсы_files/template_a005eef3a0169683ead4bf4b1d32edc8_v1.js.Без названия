$(document).on('change', '.anketa__radio--pol', function(){  
    if($(this).val() == "F"){
        $(".anketa__box--hidden").addClass("anketa__box--active");
    } else {
        $(".anketa__box--hidden").removeClass("anketa__box--active");
    }
});

