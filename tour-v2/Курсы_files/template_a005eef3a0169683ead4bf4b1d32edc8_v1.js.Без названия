    $('body').toggleClass('body--active');
});




    $(document).on('click', '.statistics__tab', function(){
        idMoth = $(this).data("tab");
        $('.statistics__tab').removeClass("statistics__tab--active");
        $(this).addClass("statistics__tab--active");
        $('.statistics__block__tab').removeClass("statistics__block__tab--active");
        $(idMoth).addClass("statistics__block__tab--active");
    });

    $(document).on('click', '.articles__choice ', function(){
        idCat = $(this).data("catid");
        $('.articles__choice').removeClass("articles__choice--active");
        $(this).addClass("articles__choice--active");
    
        $.ajax({
            type: 'POST',
            data: {
                IDCAT: idCat
            },
            success: function(html){
                $(".articles__ajax").html($(html).find(".articles"));
            }
        })
    });


  $(document).on('submit', '.video__detail__result', function(){    
        var form_data = $(this).serialize();
        $.ajax({
            type: 'POST',
            data: form_data,
            success: function(html){
                $(".result").html($(html).find(".result--ajax"));
                window.scrollTo(0,0); 
                window.location.reload();
            }
        })
        return false
   });
   
   
  $(document).on('click', '.video__items--active ', function(){    
      $(".video__items--active").removeClass("video__items--last");
      $(this).addClass("video__items--last");
      $('body').removeClass('body--actives');
      $('.video__spisok').toggleClass('video__spisok--active');
    $('.video__sticky').removeClass("video__sticky--no");
      var idUrok = $(this).data("ids");
      var idProp = $(this).data("prop");
        $.ajax({
            type: 'POST',
            data: {
				idUrok: idUrok,
                idProp: idProp
			},
            success: function(html){
                $(".video__detail").html($(html).find(".video__detail--ajax"));
            }
        })
   });


  $(document).on('click', '.courses__choice ', function(){
        idCat = $(this).data("catid");
        $('.courses__choice').removeClass("courses__choice--active");
        $(this).addClass("courses__choice--active");
        
        $.ajax({
            type: 'POST',
            data: {
                IDCAT: idCat
            },
            success: function(html){
                $(".kind__ajax").html($(html).find(".courses__list"));
            }
        })
        
    });
    
    
    $(document).on('click', '.courses__active__choice ', function(){
        $(".courses__box").css("display","block");
        
        $('.courses__active__choice').removeClass("courses__active__choice--active");
        $(this).addClass("courses__active__choice--active");
        
        elemntActive = $(this).data("tabs");
        
        if(elemntActive == "all"){
            $(".courses__box").css("display","block");
            $(".courses__box--full").css("display","block");
        } else {
            $("."+elemntActive).css("display","none");
        }
        
    });
    
    
    $(document).on('click', '.get__checkbox', function(){
        idServices = $(this).data("ids");
        nameServices = $(this).data("names");
        priceServices = $(this).data("price");
        priceFullServices = $(".get__total__value").data("pricefull");
        if($(this).prop('checked')){
            $(".get__price").append('<div class="get__shell" id="'+idServices+'"><div class="get__cost">'+nameServices+'</div><div class="get__prices">'+numberWithSpaces(priceServices)+' <span>руб</span></div></div>');
            itog = priceServices + priceFullServices;
            $(".get__total__value").text(numberWithSpaces(itog));
            $(".get__total__value").data('pricefull',itog);
        } else {
            $("#"+idServices).remove()
            itog =  priceFullServices - priceServices;
            $(".get__total__value").text(numberWithSpaces(itog));
            $(".get__total__value").data('pricefull',itog);
        }
     });
    
    function numberWithSpaces(x) {
        return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, " ");
    }


$('.change__list').owlCarousel({
    animateIn: 'fadeIn',
    animateOut: 'fadeOut',
    margin:15,
    nav:true,
    navContainer:'.change__nav',
    navText: ['<svg viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M4.167 10h11.666M10 4.167L15.833 10 10 15.833" stroke="#fff" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg>','<svg viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg"><path d="M4.167 10h11.666M10 4.167L15.833 10 10 15.833" stroke="#fff" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/></svg>'],
    dots:false,
    loop:false,
    autoWidth: false,
    autoplay:false,
    autoplayTimeout:5000, 
    responsive:{ 
        0:{
            items:1,
        },
        767:{
            items:2
        },
        1024:{
            items:2
        },
        1280:{
            items:2
        }
    }
  });


});
